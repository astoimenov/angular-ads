{
  "name": "angular-ads",
  "version": "1.3.0",
  "description": "Single page application (SPA) for Online Ads Publishing using HTML5 and AngularJS",
  "keywords": [
    "angular",
    "angularjs",
    "html5",
    "ads",
    "spa"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/astoimenov/angular-ads"
  },
  "license": "MIT",
  "dependencies": {
    "bower": "^1.3.1",
    "gulp": "^3.8.10",
